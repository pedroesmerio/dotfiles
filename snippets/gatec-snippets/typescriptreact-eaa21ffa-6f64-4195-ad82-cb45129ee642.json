{
  "Basic Gatec Form View": {
    "prefix": "basicGAtecFormView",
    "body": [
      "import React, { useEffect } from 'react';",
      "import { NativeStackScreenProps } from '@react-navigation/native-stack';",
      "import {",
      "  setScreenTitle,",
      "  confirmLeave",
      "} from 'utils';",
      "import { useI18n } from 'hooks';",
      "import { useTheme } from 'gaTheme';",
      "",
      "type ${TM_SELECTED_TEXT}Params = {",
      " // PARAMS_VIEW",
      "};",
      "",
      "type RootStackParamList = {",
      "  ${TM_SELECTED_TEXT}: ${TM_SELECTED_TEXT}Params;",
      "};",
      "",
      "type Props = NativeStackScreenProps<RootStackParamList, '$TM_SELECTED_TEXT'>;",
      "",
      "",
      "export default function $TM_SELECTED_TEXT({ navigation, route }: Props) {",
      "  const { translate, displaySnack } = useI18n();",
      "  const{ theme } = useTheme();",
      "  const { setIsEditing } = confirmLeave(navigation, theme.primary);",
      "",
      "  const {} = route.params;",
      "",
      "  setScreenTitle(translate('TITLE_VIEW'));",
      "",
      "",
      "  // #region [LOAD_DATA]",
      "  async function loadData() {",
      "    try {",
      "      // SEU LOAD DATA",
      "    } catch (error) {",
      "      displaySnack('errorLoadingData');",
      "    }",
      "  }",
      "",
      "  useEffect(() => {",
      "    const unsubscribe = navigation.addListener('focus', () => {",
      "      setTimeout(() => {",
      "        loadData();",
      "      }, 100);",
      "    });",
      "",
      "    return unsubscribe;",
      "  }, [navigation]);",
      "  //#endregion",
      "",
      "  //#region [RENDER]",
      "  return (",
      "    <>",
      "    </>",
      "  )",
      "  //#endregion",
      "}",
      ""
    ],
    "description": "Basic Gatec Form View"
  },

  "Log stringify": {
    "prefix": ["logstrfy"],
    "body": [
      "console.log('$TM_SELECTED_TEXT', JSON.stringify($TM_SELECTED_TEXT, null, 2));"
    ],
    "description": "A console.log for js obj."
  }
}
